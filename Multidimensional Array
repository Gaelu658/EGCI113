#include <stdio.h>
#define Size 3

 int main()
{
    int two_dim[3][4]={ {0,1,2,3},
                        {1,3,5,7},
                        {3,4,6,8}
                        };

    int i,j;
    for(i=0;i<3;i++){
    for(j=0;j<4;j++){
        printf("%3d",two_dim[i][j]);
        }
        printf("\n");
    }
}
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
#include <stdio.h>
#include <stdlib.h>
#define ROW_SIZE 3
#define COL_SIZE 3
void getArray(int [][COL_SIZE],int,int);
void printArray(int [][COL_SIZE],int,int);

 int main(){
    int i,j;
    int two_dim[ROW_SIZE][COL_SIZE];/*={ {0,1,2,3},
                                      {3      },
                                      {2,4    }     };*///
    getArray(two_dim,ROW_SIZE,COL_SIZE);;
    printArray(two_dim,ROW_SIZE,COL_SIZE);;

    return 0;
 }

    void getArray(int a[][COL_SIZE],int row,int col){
    int i,j;
    for(i=0;i<ROW_SIZE;i++){
        for(j=0;j<COL_SIZE;j++){
            printf("Input [%d][%d]:",i,j);
            scanf("%3d",&two_dim[i][j]);
        }

}
    for(i=0;i<ROW_SIZE;i++){
        for(j=0;j<COL_SIZE;j++){
            printf("Input [%d][%d]:",i,j);
            scanf("%3d",&two_dim[i][j]);
        }

    return 0;
}

void getArray(int a[][COL_SIZE],int row,int col){
    int i,j;
    for(i=0;i<row;i++);
    for(j=0;j<col;j++);

}
